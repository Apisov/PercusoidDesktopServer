            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <Label alignment="CENTER" contentDisplay="CENTER" text="Hits" textFill="WHITE" GridPane.halignment="LEFT" GridPane.rowIndex="2">
            <font>
                <Font name="Ubuntu Mono Bold" size="24.0" />
